{
  "expo": {
    "name": "Femtech Mobile App",
    "slug": "femtech-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "url": "https://u.expo.dev/3b709fd7-ceb1-44d6-acca-0286fcea7af4"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.frobenius.femtechmobileapp"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#007AFF",
          "sounds": [
            "./assets/images/notif.mp3"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {
            "infoPlist": {
              "NSAppTransportSecurity": {
                "NSAllowsArbitraryLoads": true
              }
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3b709fd7-ceb1-44d6-acca-0286fcea7af4"
      },
      "expoPublicWordpressConsumerKey": "ck_03fa4b83972b0bffeed920fa435b36c5ce2da7d4",
      "expoPublicWordpressConsumerSecret": "cs_dba4839b4024395a99b4d06ab554650b60dedd6b",
      "expoPublicApiServiceType": "wordpress",
      "expoPublicWordpressUrl": "https://femtech.ng/",
      "expoPublicDashboardUrl": "https://femapp.vercel.app"
    },
    "owner": "frobenius"
  }
}